 @Test(description = "Suspend user action and its checking", dependsOnMethods = {"login"})
    public void suspendUser() throws InterruptedException, IOException {
        initial.toUsersPage();
        users.search("autoAccountHO@autoAccountHO.com");
        String value = users.suspendReactivate();
        initial.logOut();
        initial.toSignPage();
        signIn.signin(readerClass.readFromFile(3),readerClass.readFromFile(4));
        signIn.suspendReactivateCheck(value);
    }

    @Test(description = "Reactivate user action and its checking", dependsOnMethods = {"suspendUser"})
    public void reactivateUser() throws IOException, InterruptedException {
        driver.navigate().refresh();
        signIn.signin(readerClass.readFromFile(1),readerClass.readFromFile(2));
        initial.toUsersPage();
        users.search("autoAccountHO@autoAccountHO.com");
        String value = users.suspendReactivate();
        initial.logOut();
        initial.toSignPage();
        signIn.signin(readerClass.readFromFile(3),readerClass.readFromFile(4));
        signIn.suspendReactivateCheck(value);

    }
